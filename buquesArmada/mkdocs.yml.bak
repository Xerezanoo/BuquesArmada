site_name: Armada Española (2025)

nav:
  - ./index.md
  - 01. Fragatas:
    - ./01. Fragatas/index.md
    - 01. Clase Álvaro de Bazán (F-100):
      - ./01. Fragatas/01. Clase Álvaro de Bazán (F-100)/index.md
    - 02. Clase Santa María (F-80):
      - ./01. Fragatas/02. Clase Santa María (F-80)/index.md
  - 02. Buques Anfibios:
    - ./02. Buques Anfibios/index.md
  - 03. Buques Logísticos:
    - ./03. Buques Logísticos/index.md
  - 04. Buques Medidas contra Minas:
    - ./04. Buques Medidas contra Minas/index.md
  - 05. Patrulleros y Buques de Cooperación Pesquera:
    - ./05. Patrulleros y Buques de Cooperación Pesquera/index.md
  - 06. Buques de Acción Marítima:
    - ./06. Buques de Acción Marítima/index.md
  - 07. Buques Auxiliares:
    - ./07. Buques Auxiliares/index.md
  - 08. Buques Hidrográficos:
    - ./08. Buques Hidrográficos/index.md
  - 09. Buque de Investigación Oceanográfica:
    - ./09. Buque de Investigación Oceanográfica/index.md
  - 10. Buques Escuela:
    - ./10. Buques Escuela/index.md

theme:
    features:
        - navigation.indexes

    language : es

    name: 'material'

# Extensions
markdown_extensions:
  - footnotes
  # - attr_list
  - pymdownx.arithmatex:
      generic: true
  # - pymdownx.superfences
  # - pymdownx.details
  # - pymdownx.magiclink
  - pymdownx.tasklist:
      custom_checkbox: true
  - def_list
  - pymdownx.critic
  - pymdownx.caret
  - pymdownx.keys
  - pymdownx.mark
  - pymdownx.tilde

  - toc:
      permalink: true

plugins:
  - search
  - roamlinks
  - mermaid2:
      javascript: https://unpkg.com/mermaid/dist/mermaid.min.js

extra_javascript:
  - javascripts/mathjax.js
  - https://polyfill.io/v3/polyfill.min.js?features=es6
  - https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-mml-chtml.js

